/* CSS Document */
body{
  color: #333333;  
  font-family: Lucida Sans Unicode, Lucida Grande, sans-serif; 
}
p{
  margin: 0 !important;
}
a{
  color: #000000;
}
a:hover{
  text-decoration: underline;
}
#header{
  width: 950px;
  background: #000099;
  height: 204px;
}

  #header .top_header{
    float: left;
    width: 950px;
    height: 28px;
    background: url(../img/layout/top_header.png) no-repeat;
  }
     #header .top_header ul{
          text-align: right;
          line-height: 25px;
          margin-right: 16px;
      }
          #header .top_header ul li{
            display: inline;
            padding-left: 13px;
            padding-right: 13px;
            border-right: 1px solid #ccc;
            height: 10px;
          }
            #header .top_header ul li a{
              font-size: 11px;
              color:#333333;
              text-decoration: none;
          }
              #header .top_header ul li a:hover{
                font-size: 11px;
                color:#333333;
                text-decoration: underline;
          }
              #header .top_header ul li a.actual{
                color:#AC0CA5;
              }
  #header .center_header{
    float: left;
    width: 950px;
    height: 132px;
    background: #fff;
  }
      #header .center_header .logo_header{
        float: left;
        width: 186px;
        height: 132px;
        margin-left: 21px;
      }
      #header .center_header .foto_header{
        float: left;
        width: 389px;
        height: 132px;
        background: url(../img/layout/foto_header.png) no-repeat;
        margin-left: 45px;
      }
        #header .center_header .foto_header .imagen{
          position:relative;
          left:31px;
        }
    
    #header .contenedor_app{
      float: left;
      width: 309px;
      height: 132px;
    }    
       #header .contenedor_app .acceso_usuarios{
          float: left;
          width: 243px;
          height: 89px;
          background: url(../img/layout/fondo_acceso_usuarios.png) no-repeat;
          padding-left:66px;
          padding-top:2px;
        } 
          #header .contenedor_app .acceso_usuarios p{
            float: left;
            width: 207px;
            height: 22px;
            
        }
            #header .contenedor_app .acceso_usuarios .text_acc_us{
              font-size: 12px;
              color: #AC0CA5;
              font-weight: bold;
              line-height:20px;
            }
              #header .contenedor_app .acceso_usuarios input[type="text"],
              #header .contenedor_app .acceso_usuarios input[type="password"]{
                width: 180px;
                margin:0 0 2px 15px;
              }           
              #header .contenedor_app .acceso_usuarios label{
                font-size:11px;
                font-weight: normal;
                color: #000000;
                display:block;
                width:30px;
                float:left;
              }
              #header .contenedor_app .acceso_usuarios input[type="submit"]{
                margin-left: 173px;
                background: url(../img/layout/boton_enviar_acc.png) no-repeat;
                border:0 none;
                font-size:10px;
                height:29px;
                padding-right:11px;
                width:64px;
                margin-top:-3px;
              }
       #header .contenedor_app .buscador{
          float: left;
          width: 309px;
          height: 41px;
          background: url(../img/layout/fondo_buscador.png) no-repeat;
          padding-left:13px;
        } 
           #header .contenedor_app .buscador input[type="text"]{
                background:url("../img/layout/buscador.png") no-repeat scroll 0 0 transparent;
                border:medium none;
                font-size:10px;
                height:28px;
                margin:12px 0 0 85px;
                padding-left:5px;
                padding-right:30px;
                width:160px;
                color: #333333;
              }
              
              
  #header .bottom_header{
    float: left;
    width: 950px;
    height: 44px;
    background: url(../img/layout/fondo_botonera.png) no-repeat;
  }
     #header .bottom_header ul{
        display: block;
        margin-left:5px;
        padding-top:12px;
  }
      #header .bottom_header ul li{
        display:block;
        float:left;
        font-size:14px;
        margin-left:10px;
        
  }
        #header .bottom_header ul li a{
         text-decoration: none;
         line-height:30px;         
         padding-right:17px;
         padding-left:27px;
         font-weight: bold;
         display:block; 
  }
        #header .bottom_header ul li a span{
          display:block;
          height:29px;
        }

        #header .bottom_header ul li:hover{
          background: url(../img/layout/tab-bot-left.png) no-repeat;
        }
        #header .bottom_header ul li a:hover span {
          background-color: #61BE1A;
        }
        #header .bottom_header ul li a:hover{
          background: url(../img/layout/tab-bot-right.png) no-repeat right;
          color:#fff;
        }
  li.btn_destacado a{
    color: #AC0CA5;
  }
  li.btn_destacado {
    background: url(../img/layout/star-bot-left.png) no-repeat;
  }
  li.btn_resto {
    background: url(../img/layout/star-bot-left_2.png) no-repeat;
  }
   
        #header .bottom_header ul li.actual{
          background: url(../img/layout/tab-bot-left.png) no-repeat;
        }
        #header .bottom_header ul li.actual a span {
          background-color: #61BE1A;
        }
        #header .bottom_header ul li.actual a{
          background: url(../img/layout/tab-bot-right.png) no-repeat right;
          color:#fff;
        }

/*CONTENIDO HOME*/

#contenido{
  background: url(../img/layout/fondo_content.png) repeat-y;
  padding:10px 0 0 54px;
  overflow:hidden;
} 

  #contenido .nivel_uno{
    display:block;
    overflow:hidden;
    width:880px;
    border:1px solid #B3ADA5;
    padding:8px 0 4px 0;
  }
    #contenido .nivel_uno .noticia{
      display:block;
      float:left;
      width:210px;
      margin-left:8px;
      overflow:hidden;
      position:relative;
    }
      #contenido .nivel_uno .noticia img{
        z-index:2;
      }
      #contenido .nivel_uno .noticia .descripcion{
        background: url(../img/layout/fondo_noticia_home.png) repeat-y;
        position:absolute;
        left:0;
        top:123px;
        width:199px;
        height:108px;
        padding:5px;
        z-index:999;
      }
      #contenido .nivel_uno .noticia .descripcion h2,
      #contenido .nivel_uno .noticia .descripcion p a,
      #contenido .nivel_uno .noticia .descripcion p,
      #contenido .nivel_uno .noticia .descripcion h2 a{
        color:#fff;
        text-decoration:none;
        padding:0px;
        margin:0px;        
      }
      #contenido .nivel_uno .noticia .descripcion h2 a:hover{
        color:#B3ADA5;
      }
      #contenido .nivel_uno .noticia .descripcion h2{
        font-size:18px;
      }
      #contenido .nivel_uno .noticia .descripcion p{
        font-size:12px;
      }

  #contenido .nivel_dos{
    float:none;
    clear:both;
    margin:20px 0;
    overflow:hidden;  
  }
    #contenido .nivel_dos .noticias, #contenido .nivel_dos .destacados{
      width:320px;
      float:left;
      padding:0 13px 0 0;
      margin:0 12px 0 0;
      display:block;
      border-right:1px dotted #C6C0BB;
      overflow:hidden;
    }
      #contenido .nivel_dos .noticias ul, #contenido .nivel_dos .destacados ul{
        list-style: none;
        margin:0px;
        padding:0px;
      }
      #contenido .nivel_dos .noticias ul li, #contenido .nivel_dos .destacados ul li{
        padding:10px 0;
        border-bottom:1px solid #C6C0BB;
        overflow:hidden;
      }
        #contenido .nivel_dos .destacados ul li h2, #contenido .nivel_dos .noticias ul li h2{
          font-size:12px;
          font-weight:bold;
          margin:0px;
          padding:0px;
          line-height:12px;
        }
        #contenido .nivel_dos .destacados ul li h2 a{
          color:#AC0CA5;
          text-decoration:none;
        }
        #contenido .nivel_dos .destacados ul li h2 a:hover{
          color:#AC0CA5;
          text-decoration:underline;
        }
                  
    #contenido .nivel_dos .titulo_noticias, #contenido .nivel_dos .titulo_destacados{
      display:block;
      padding:4px 5px 10px 10px;
      background: url(../img/layout/fondo_secciones.png) no-repeat;
      color:#fff;
      font-size:14px;
    }

    #contenido .nivel_dos .noticias .fecha{
      float:left;
      width:36px;
      padding:0 10px 0 0;
      font-weight:bold;
      text-align:center;
    }
    #contenido .nivel_dos .noticias .informacion{
      display:block;
      float:left;
      width:274px;
    }
      #contenido .nivel_dos .noticias .fecha span{
        display:block;
        padding:0px;
        margin:0px;
        line-height:14px;
      }
    
      #contenido .nivel_dos .noticias .fecha .mes{
        font-size:15px;
        text-transform:uppercase;
      }
      #contenido .nivel_dos .noticias .fecha .dia{
        font-size:24px;
        line-height:24px;
      }
      #contenido .nivel_dos .noticias .fecha .ano{
        font-size:12px;
      }
  #contenido .nivel_dos .banners, #contenido .banners{
    float:left;
    display:block;
    overflow:hidden;
  }
    #contenido .nivel_dos .banners ul, #contenido .banners ul{
      list-style:none;
      padding:0px;
      margin:0px;
    }
    #contenido .nivel_dos .banners ul li, #contenido .nivel_dos .banners ul li a, #contenido .banners ul li, #contenido .banners ul li a{
      margin:0 0 10px 0;
      padding:0px;
    }
    #contenido .nivel_dos .banners ul li a img, #contenido .banners ul li a img{
      padding:0px;
      border:0px;
      margin:0px;
    }
  #contenido .lateral{
    float:right;
    margin-right:14px;
  }


/*FOOTER */

#footer{
  clear:both;
  float:none;
}
  #footer .footer_informacion{
    background: url(../img/layout/fondo_content.png) repeat-y;
    float:left;
    width:952px;  
}  

    #footer .footer_informacion .col_uno{
      width: 105px;
      float: left;
      height: 100px;
      margin-left: 60px;
      border-top: 5px solid #AC0DA5;
}    
      #footer .footer_informacion .col_uno ul{
        padding: 0;
        width: 105px;
        margin-top: 10px;
}    
        #footer .footer_informacion .col_uno ul li{
        padding: 0;
        list-style: none;
        border-bottom: 1px dotted #ccCCCC;
        width: 105px;
        padding-left: 5px;
}       
          #footer .footer_informacion .col_uno ul li a{
            color: #333333;
            font-size: 12px;
            text-decoration: none;
          }
            #footer .footer_informacion .col_uno ul li a:hover{
              text-decoration: underline;
          }
    #footer .footer_informacion .col_dos{
      width: 132px;
      float: left;
      height: 100px;
      padding-left: 60px;
      border-top: 5px solid #AC0DA5;
}    
      #footer .footer_informacion .col_dos ul{
        padding: 0;
        margin-top: 10px;
        width: 132px;
      }    
        #footer .footer_informacion .col_dos ul li{
          padding: 0;
          list-style:none;
          border-bottom: 1px dotted #ccCCCC;
          padding-left: 5px;
        }  
          #footer .footer_informacion .col_dos ul li a{
            color: #AC0CA5;
            font-size:12px;
            text-decoration: none;
          }  
            #footer .footer_informacion .col_dos ul li a:hover{
                text-decoration: underline;
            }
            
.caja_footer{
   color: #AC0CA5;
   font-size:12px;
   text-decoration: none;
   border-bottom:none !important;
   background: #EECFED;
   height: 17px;
   padding-bottom: 3px !important;
   
}
.direccion{
  border-right: 1px dotted #cccCCC;
  height: 70px;
  padding-right:20px;
}
  #footer .footer_informacion .col_tres{
    border-top: 5px solid #AC0DA5;
    float:left;
    margin-right:0;
    padding-top:30px;
    text-align:right;
    width:476px;
    
}    
    #footer .footer_informacion .col_cuatro{
      float: left;
      border-top: 5px solid #AC0DA5;
      text-align: right;
      padding-top: 30px;
      width:85px;
}    

  #footer .footer_huincha{
    background: url(../img/layout/footer_huincha.png) no-repeat;
    width:952px;  
    height: 50px;
    float: left;
}

.linea_footer{
    border-top: 5px solid #AC0DA5;
    /*padding-left: 500px;
    padding-top: 5px;*/
}



/* ***************** QUIENES **************/
#ruta{
  padding:0 0 10px 0;
}
  #ruta p{
    color:#666;
  }
    #ruta p a{
      color:#333;
      text-decoration:none;
    }
    #ruta p a:hover{
      text-decoration:underline;
    }

#principal{
  float:left;
  overflow:hidden;
  margin-bottom:20px;
  min-height:400px
}
#principal.tres{
  width:461px;
  display:block;
}
#principal.dos{
  width:672px;
  display:block;
}
  #principal h2{
    color:#4D4B4A;
    font-size:22px;
  }
  #principal p{
    color:#333;
  }
#principal .noticias, #principal .buscador{
  overflow:hidden;
  margin-bottom:40px;
}
  #principal h1{
    font-size:22px;
    color:#AC0CA5;
    font-weight:bold;
  }
  #principal .noticias h3, #principal .buscador h3{
    font-size:13px;
    color:#333;
    margin:0px;
  }
  #principal .noticias h2, #principal .buscador h2{
    font-size:21px;
    color:#333;
    line-height:21px;
    margin:0 0 15px 0;
    padding:0px;
  }
  #principal .buscador ul.resultados{
    list-style:decimal;
    margin-top:20px;
    padding-left:20px;
  }
    #principal .buscador ul.resultados li{
      margin-bottom:20px;
    }
    #principal .buscador ul.resultados li h3, #principal .buscador ul.resultados li h3 a{
      color:#AC0CA5;
      font-size:14px;
      text-decoration:none;
      font-weight:bold;
    }
    #principal .buscador ul.resultados li h3 a:hover{
      text-decoration:underline;
    }
    #principal h3{
      color:#4C4B4A;
      font-size:16px;
      padding:0px;
      margin:25px 0 5px 0;
    }
    #principal ul.links {
      list-style:none;
      margin:0px;
      padding:0px;
    }
    #principal ul.links li {
      margin:0px;
    }
    #principal ul.links li a{
      text-decoration:none;
      color:#333;
    }
    #principal ul.links li a:hover{
      text-decoration:underline;
    }
    
.navBuscador{
  display:block;
  border-top:1px solid #C6C0BB;
  padding-top:10px;
  padding-bottom:50px;
  width:875px;
  float:none;
  clear:both;
}
  .navBuscador span{
    display:block;
    float:left;
    padding-left:50px;
  }
  .navBuscador span a{
    color:#AC0CA5;
  }
#principal .recientes{
  overflow:hidden;
  border-top:1px dotted #C6C0BB;
  padding-top:10px;
}
  #principal .recientes .bloqueNot{
    display:block;
    float:left;
    width:202px;
    padding:10px;
    margin:0;
    border-right:1px solid #C6C0BB;
    height:100%;
  }
  #principal .recientes .last{
    border-right:0;
  }
    #principal .recientes .bloqueHorizontal{
      border-bottom:1px dotted #C6C0BB;
      min-height:50px;
      overflow:hidden;
      padding: 10px 0;
      margin-left:-10px;
    }
  #principal .galCal{
    float:left;
    margin:0 10px 10px 0;
    height:155px;
  }
    #principal .galCal p{
      display:block;
      height:16px;
      padding:2px 31px 0px 6px;
      margin:0;
      width:156px;
      line-height:12px;
    }
    #principal .galCal a img{
      padding:5px;
      border:1px solid #C6C0BB;
    }
    #principal .foto p{
      background:url(../img/layout/ico_imagen.gif) no-repeat right center;
    }
    #principal .video p{
      background:url(../img/layout/ico_video.gif) no-repeat right center;
    }
#secundario{
  width:191px;
  padding:0 20px 0 0;
  float:left;
  overflow:hidden;
  margin-bottom:20px;
}
  #secundario h3{
    color:#4D4B4A;
    font-size:14px;
    font-weight:bold;    
  }
  #secundario.destacados h3{
    color:#AC0CA5;
  }
  #secundario .caluga{
    display:block;
    padding:5px 50px 5px 5px;
    border:1px solid #C6C0BB;
    margin:15px 0 0 0;
  }
  #secundario .galeria{
    background:url(../img/layout/caluga_galeria_bg.png) no-repeat right bottom;
  }
  #secundario .prodecimientos{
    background:url(../img/layout/caluga_procedimientos_bg.png) no-repeat right bottom;
  }
    #secundario .caluga h3{
      color:#AC0CA5;
      margin-bottom:5px;
    }
  #secundario ul{
    list-style:none;
    margin:0px;
    padding:0px;
  }
    #secundario ul li{
      padding:0 0 0 15px;
      background:url(../img/layout/bullet-arrow.png) no-repeat left 6px;
    }
      #secundario ul li a{
        text-decoration:none;
        color:#333333;
      }
      #secundario ul li a:hover{
        text-decoration:underline;
      }
      
#testimonio{
  float:left;
  width:190px;
  display:block;
  overflow:hidden;
  margin-bottom:20px;
  padding-left:20px;
}
  #testimonio ul{
    list-style:none;
    padding:0px;
    margin:0px;
  }
    #testimonio ul li{
      padding:6px 6px 20px 6px;
      border:1px solid #C6C0BB;
      display:block;
      width:176px;
      margin:0px;
    }
      #testimonio ul li h3{
        color:#AC0CA5;
        font-size:12px;
        line-height:14px;
        margin:0 0 5px 0;
        font-weight:bold;
        text-transform:uppercase;
      }
      #testimonio ul li p{
        color:#333;
        line-height:14px;
      }

/*************** MAPA DEL SIITO ****************/

#principal.mapa h3, #principal.mapa ul li h3{
  padding:0 0 0 25px;
  margin:15px 0 5px 0;
  font-size:13px;
  color:#333;
}
#principal.mapa ul li h3{
  list-style:none;
}
#principal.mapa ul{
  list-style:none;
  padding:0px;
  margin:0px;
}
#principal.mapa ul li{
  margin:0px;
  padding:0px;
}
#principal.mapa ul li ul{
  list-style:none;
  margin:0 0 0 10px;
}
#principal.mapa ul a, #principal.mapa h3 a{
  text-decoration:none;
  color:#333;
}
#principal.mapa ul a:hover, #principal.mapa h3 a:hover{
  text-decoration:underline;
}
#principal.mapa div.bloque_izq, #principal.mapa div.bloque_der{
  display:block;
  float:left;
  width:49%;
}
#principal.mapa h3{
  background:url(../img/layout/ico_folder.gif) left center no-repeat;
}
#principal.mapa ul li ul li{
  background:url(../img/layout/ico_doc.gif) no-repeat center left;
  padding-left:25px;
  margin-left:25px;
}
#principal.mapa h3.inicio{
  background:url(../img/layout/ico_home.gif) no-repeat left center;
}
#principal.mapa h3.reporte{
  background:url(../img/layout/ico_reporte.gif) no-repeat left center;
}

/********************** FORMULARIO DE CONTACTO *******************/

div.contact-form{
  padding-top:20px;
  overflow:hidden;
  display:block;
}
div.contact-form div.bloque_izq{
  float:left;
  width:40%;
}
div.contact-form div.bloque_der{
  float:right;
  width:57%;
}
div.contact-form div.submit{
  display:block;
  float:none;
  clear:both;
  padding:20px 0 0 80px;
}
div.contact-form div label, div.contact-form span {
  width:80px;
  float:left;
  display:block;
  line-height:26px;
  color:#333;
  font-weight:normal;
  font-size:13px;
}
div.contact-form div.select select, div.contact-form div input{
  width:180px;
  line-height:26px;
  font-size:13px;
}

div.contact-form div.textarea label{
  float:none;
  display:block;
}
div.contact-form div.textarea textarea#Comentarios{
  width:90%;
  font-size:13px;
  line-height:16px;
  height:114px;
  font-family:Lucida Sans Unicode, Lucida Grande, sans-serif;  
}
div.contact-form div.submit input{
  width:60px;
  text-align:center;
  font-size:12px;
}
